{
    "name": "marqu3s/yii2-sweet-alert",
    "description": "Alert widget based on SweetAlert extension {@link http://tristanedwards.me/sweetalert)",
    "type": "yii2-extension",
    "keywords": ["yii2", "module"],
    "homepage": "https://github.com/marqu3s/yii2-sweet-alert",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Igor Chepurnoy",
            "email": "igorzfort@gmail.com"
        },
        {
            "name": "Joao Marques",
            "email": "joao@jjmf.com"
        }
    ],
    "require": {
        "bower-asset/sweetalert" : "*",
        "yiisoft/yii2": "*"
    },
    "autoload": {
        "psr-4": {
            "marqu3s\\sweetalert\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
